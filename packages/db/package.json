{
  "name": "@khaledaun/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./client.ts"
  },
  "scripts": {
    "generate": "prisma generate --schema=./schema.prisma",
    "push": "prisma db push --schema=./schema.prisma",
    "migrate": "prisma migrate dev --schema=./schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=./schema.prisma",
    "studio": "prisma studio --schema=./schema.prisma",
    "seed": "tsx seed.ts",
    "reset": "prisma migrate reset --schema=./schema.prisma",
    "postinstall": "prisma generate --schema=./schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.5.0"
  },
  "devDependencies": {
    "prisma": "^5.5.0",
    "tsx": "^3.14.0"
  }
}